{
  "name": "eco",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "check:tls": "node tools/eco-tls-check.mjs",
    "db:apply": "node tools/eco-db-apply.mjs",
    "seed:eco": "node tools/eco-seed.mjs",
    "cleanup:dryrun": "node tools/eco-cleanup-dryrun.mjs",
    "smoke:beta": "node tools/eco-smoke-beta.mjs",
    "test:rls": "node tools/eco-rls-proof.mjs",
    "test:pack": "npm run db:apply && node tools/eco-create-test-users.mjs && node tools/eco-rls-proof.mjs",
    "test:ui": "playwright test",
    "verify:all": "npm run lint && npm run build && npm run test:pack",
    "verify:ops": "npm run check:tls && npm run verify:all && npm run test:ui",
    "verify:beta": "npm run verify:ops && npm run smoke:beta"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.97.0",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "pg": "^8.19.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.3.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "typescript": "^5"
  }
}
